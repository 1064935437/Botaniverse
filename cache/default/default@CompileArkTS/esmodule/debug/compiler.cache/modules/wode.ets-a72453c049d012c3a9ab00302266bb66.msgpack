    .height(56)
    .padding({ left: 16, right: 16 })
    .justifyContent(FlexAlign.SpaceBetween)
    .backgroundColor('#FFFFFF')
  }

  aboutToAppear() {
    // 每次页面显示时更新用户名
    this.nickname = userStore.getUsername() || '未设置昵称'
