    Row() {
      Row({ space: 12 }) {
        Image(icon)
          .width(24)
          .height(24)
        Text(title)
          .fontSize(16)
          .fontColor('#333333')
      }

      if (arrow) {
        Image($r('app.media.ic_arrow_right'))
          .width(20)
          .height(20)
          .fillColor('#999999')
